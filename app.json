{
  "expo": {
    "name": "airbnb",
    "slug": "airbnb",
    "version": "1.1.0",
    "orientation": "portrait",
    "githubUrl": "https://github.com/JConan/react-native_airbnb",
    "description": "Exercice @LeReacteurIO",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.jconan.airbnb",
      "supportsTablet": true
    },
    "android": {
      "package": "com.jconan.airbnb",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "sdkVersion": "42.0.0"
  }
}